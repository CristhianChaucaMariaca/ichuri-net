mixin social
    .social
        a(href=facebook, target="_blank").icon-facebook.giro-1
        a(href="#").icon-whatsapp.giro-2
        a(href="#").icon-youtube.giro-3
        a(href="#").icon-mail2.giro-4
mixin titulo(titulo)
    .title-general
        i(class=attributes.class)
        h2.section-title.title-2 #{titulo}